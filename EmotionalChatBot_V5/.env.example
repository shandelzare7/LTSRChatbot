OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o
# If you use a non-OpenAI provider (OpenAI-compatible), set:
# OPENAI_BASE_URL=https://your-provider.example.com/v1

# ------------------------------------------
# Multi-LLM routing presets (recommended)
# ------------------------------------------
# openai:        main=gpt-4o, fast=gpt-4o-mini, judge=gpt-4o
# deepseek_route_a: main=DeepSeek, fast/judge=OpenAI (best quality/perf mix)
# deepseek_route_b: all=DeepSeek (single provider)
LTSR_LLM_PRESET=openai

# For deepseek_route_a / deepseek_route_b
DEEPSEEK_API_KEY=
# For openai / route_a fast+judge
OPENAI_API_KEY_OPENAI=

# Optional per-role overrides (highest priority):
# LTSR_LLM_MAIN_BASE_URL=...
# LTSR_LLM_MAIN_MODEL=...
# LTSR_LLM_MAIN_API_KEY=...
# LTSR_LLM_FAST_MODEL=gpt-4o-mini
# LTSR_LLM_JUDGE_MODEL=gpt-4o

# ------------------------------------------
# LATS tuning (balanced defaults)
# ------------------------------------------
# Keep soft scorer enabled (do NOT turn this off in production)
LATS_ENABLE_LLM_SOFT_SCORER=1
LANGCHAIN_TRACING_V2=0
LANGCHAIN_API_KEY=
LANGCHAIN_PROJECT=LTSRChatbot-prod
# PostgreSQL (prod, local on server)
DATABASE_URL=postgresql+asyncpg://chatbot_user:CHANGE_ME_STRONG_PASSWORD@localhost:5432/ltsrchatbot_v5

